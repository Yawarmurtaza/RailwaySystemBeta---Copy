<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
    </startup>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="BasicHttpBinding_ILoginService" />
                <binding name="BasicHttpBinding_IPasagerService" />
                <binding name="BasicHttpBinding_IServerService" />
                <binding name="BasicHttpBinding_ISignUpService" />
                <binding name="BasicHttpBinding_IAdminService" />
            </basicHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost:8733/Design_Time_Addresses/ServerWCF/LoginService/"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_ILoginService"
                contract="LoginServiceReference.ILoginService" name="BasicHttpBinding_ILoginService" />
            <endpoint address="http://localhost:8733/Design_Time_Addresses/ServerWCF/PasagerService/"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IPasagerService"
                contract="PasagerServiceReference.IPasagerService" name="BasicHttpBinding_IPasagerService" />
            <endpoint address="http://localhost:8733/Design_Time_Addresses/ServerWCF/ServerService/"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IServerService"
                contract="ServerServiceReference.IServerService" name="BasicHttpBinding_IServerService" />
            <endpoint address="http://localhost:8733/Design_Time_Addresses/ServerWCF/SignUpService/"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_ISignUpService"
                contract="SignUpServiceReference.ISignUpService" name="BasicHttpBinding_ISignUpService" />
            <endpoint address="http://localhost:8733/Design_Time_Addresses/ServerWCF/AdminService/"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IAdminService"
                contract="AdminServiceReference.IAdminService" name="BasicHttpBinding_IAdminService" />
        </client>
    </system.serviceModel>
</configuration>